    "@oclif/config": "^1.8.8",
    "@types/fs-extra": "^5.0.4",
    "cli-ux": "^4.7.3",
    "diff": "^3.5.0",
    "fs-extra": "^7.0.0",
    "inquirer": "^6.0.0",
    "jsdiff": "^1.1.1",
    "lodash": "^4.17.11",
    "randomstring": "^1.1.5",
    "tslib": "^1"
  },
  "devDependencies": {
    "@fancy-test/nock": "^0.1.1",
    "@oclif/config": "^1.8.8",
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^1",
    "@oclif/test": "^1.1.0",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4",
    "@types/diff": "^3.5.1",
    "@types/execa": "^0.9.0",
    "@types/glob": "^5.0.35",
    "@types/inquirer": "^0.0.41",
    "@types/mocha": "^5",
    "@types/nock": "^9.3.0",
    "@types/node": "^10",
    "@types/randomstring": "^1.1.6",
    "@types/supports-color": "^5.3.0",
    "@types/write-json-file": "^2.2.1",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nock": "^10.0.0",
    "nyc": "^11",
    "sinon": "^6.3.2",
    "stub": "^0.3.2",
    "ts-node": "^7.0.1",
    "tslint": "^5",
    "tsutils": "2.27.2",
    "typescript": "3.5.3"
  },
  "engines": {
    "node": "16.x"
  },
  "files": [
    "/lib",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/heroku/heroku-cli-addons-admin",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "heroku",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "heroku/heroku-cli-addons-admin",
  "scripts": {
    "clean:branch": "./bin/clean-branch.sh",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
