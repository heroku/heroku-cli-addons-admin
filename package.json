{
  "name": "@heroku-cli/plugin-addons-admin",
  "description": "Heroku CLI plugin to help Heroku add-on providers integrate their services with Heroku.",
  "version": "2.3.2",
  "author": "Heroku",
  "bugs": "https://github.com/heroku/heroku-cli-addons-admin/issues",
  "dependencies": {
    "@heroku-cli/color": "^2.0.4",
    "@heroku-cli/command": "^8.5.0",
    "@heroku-cli/schema": "^1.0.3",
    "@oclif/command": "^1.8.36",
    "@oclif/config": "^1.18.17",
    "@oclif/plugin-help": "^6.2.14",
    "cli-ux": "^5.2.1",
    "diff": "^3.5.0",
    "fs-extra": "^9.1.0",
    "http-call": "5.3.0",
    "inquirer": "^8.2.6",
    "jsdiff": "^1.1.1",
    "lodash": "^4.17.21",
    "randomstring": "^1.3.0",
    "tslib": "2.6.3"
  },
  "devDependencies": {
    "@heroku-cli/dev-cli": "^1.26.13",
    "@oclif/test": "^2.5.6",
    "@types/chai": "^4",
    "@types/diff": "^3.5.1",
    "@types/execa": "^0.9.0",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "8.1.0",
    "@types/inquirer": "^8.2.10",
    "@types/mocha": "^10.0.7",
    "@types/nock": "^9.3.0",
    "@types/node": "^10.9.4",
    "@types/randomstring": "^1.3.0",
    "@types/supports-color": "^5.3.0",
    "@types/write-json-file": "^2.2.1",
    "chai": "^4",
    "eslint": "^7",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^10",
    "mocha": "^10.7.3",
    "nock": "^13.5.4",
    "nyc": "^15.1.0",
    "sinon": "^18.0.0",
    "ts-node": "^10.9.2",
    "tsheredoc": "^1.0.1",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">= 16.0"
  },
  "files": [
    "/lib",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/heroku/heroku-cli-addons-admin",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "mocha": {
    "require": [
      "test/helpers/init.js",
      "ts-node/register",
      "source-map-support/register"
    ],
    "watch-extensions": "ts",
    "recursive": true,
    "reporter": "spec",
    "timeout": 5000
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "heroku",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "heroku/heroku-cli-addons-admin",
  "scripts": {
    "clean:branch": "./bin/clean-branch.sh",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && eslint .",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
